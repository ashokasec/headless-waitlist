{
  "name": "headless-waitlist",
  "version": "0.1.0",
  "private": true,
  "author": "ashokasec",
  "scripts": {
    "COMMENT__1": "DEFAULT SCRIPTS ----------------------------",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    ".": ".",
    "COMMENT__2": "BIOME SCRIPTS -------------------------------",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint --write .",
    "biome:check": "biome check --write .",
    "..": "..",
    "COMMENT__3": "BIOME SCRIPTS -------------------------------",
    "shadcn:add": "pnpm dlx shadcn@latest add",
    "...": "...",
    "COMMENT__4": "DRIZZLE-ORM SCRIPTS -------------------------",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "....": "....",
    "COMMENT__5": "DOCEKR SCRIPTS ------------------------------",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down --volumes --remove-orphans"
  },
  "dependencies": {
    "@arcjet/next": "1.0.0-beta.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "resend": "^4.6.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  }
}
